@using System.Globalization
<header id="page-title" class="ng-scope affix">
    <h1 class="pull-left">@ViewData["Culture"].ToString()  - @(new CultureInfo(@ViewData["Culture"].ToString()).DisplayName)  <span>module list</span></h1>
</header>
<section id="main-content" style="margin-top: 47px;" class="ng-scope">
    <div id="page-actions" class="clearfix">
        <div btn-actions>
            <button class="btn btn-small" data-ng-click="exit()">@T("Back")</button>
            <button class="btn btn-small" data-ng-click="getAllMetadata()">
                <i class="icon-refresh"></i>&nbsp;@T("Refresh")
            </button>
            <button class="btn btn-small" data-ng-click="view(selectedItems[0])" ng-disabled="!(selectedItems.length == 1)">
                <i class="icon-edit"></i>&nbsp;View
            </button>
            <button class="btn btn-small" data-ng-click="edit(selectedItems[0])" ng-disabled="!(selectedItems.length == 1)">
                <i class="icon-edit"></i>&nbsp;Edit
            </button>
         </div>
    </div>

    <div class="row-fluid" data-ag-grid="gridOptions" data-ag-grid-name="gridList">
    </div>
</section>
