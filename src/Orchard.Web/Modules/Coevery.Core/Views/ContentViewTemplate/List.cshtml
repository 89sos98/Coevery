<section data-ng-controller="GeneralListCtrl">
    <div id="page-actions">
        <button class="btn btn-small btn-success" data-ng-click="add()">
            <i class="icon-plus"></i>&nbsp;Add
        </button>
        <button class="btn btn-small">
            <i class="icon-refresh"></i>&nbsp;Refresh
        </button>
        <div class="btn-group">
            <button class="btn btn-small">Follow</button>
            <button class="btn btn-small">Edit</button>
            <button class="btn btn-small">Delete</button>
        </div>
        <div class="btn-group">
            <button class="btn btn-small dropdown-toggle" data-toggle="dropdown">
                More
                <span class="caret"></span>
            </button>
            <ul class="dropdown-menu">
                <li><a href="#">Convert</a></li>
                <li><a href="#">Disabled</a></li>
            </ul>
        </div>
    </div>

    <section id="main-content">
        <header id="page-title">
            <h1>{{moduleName}} <span>show all data</span></h1>
        </header>

        <div class="row-fluid">
            <div class="btn-toolbar clearfix">
                <div class="btn-group">
                    <button class="btn btn-small dropdown-toggle" data-toggle="dropdown">
                        <span>过滤器</span>
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li title="" style="cursor: pointer;">
                            <a ng-click="openFilterCollapse(1)">价格过滤<div style="float: right; text-decoration: underline;">删除</div>
                            </a>
                        </li>
                        <li title="" style="cursor: pointer;">
                            <a ng-click="openFilterCollapse(2)">价格过滤<div style="float: right; text-decoration: underline;">删除</div>
                            </a>
                        </li>
                        <li title="" style="cursor: pointer;">
                            <a ng-click="openFilterCollapse(3)">价格过滤<div style="float: right; text-decoration: underline;">删除</div>
                            </a>
                        </li>
                        <li title="">
                            <hr style="margin: 0px;" />
                        </li>
                        <li title="" style="cursor: pointer;"><a>创建过滤器</a>
                        </li>
                    </ul>
                </div>

                <div class="btn-group">
                    <button class="btn btn-small dropdown-toggle" data-toggle="dropdown">
                        <span>View</span>
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu" style="">
                        <li title="" style="cursor: pointer;"><a>创建View</a>
                        </li>
                        <li title="">
                            <hr style="margin: 0px;" />
                        </li>
                        <li title="" style="cursor: pointer;">
                            <a>List View<div style="float: right; text-decoration: underline;">删除</div>
                            </a>
                        </li>
                        <li title="" style="cursor: pointer;">
                            <a>Chart View<div style="float: right; text-decoration: underline;">删除</div>
                            </a>
                        </li>
                    </ul>
                </div>

                <div class="input-append" style="margin: 0px 0px 0px 10px;">
                    <input type="text" style="width: 150px; height: 16px; line-height: 16px; font-size: 13px;">
                    <button type="button" class="btn btn-small">
                        <i class="icol-magnifier"></i>
                    </button>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div id="filterCollapse" class="accordion" style="margin-top: 10px; margin-bottom: 10px; display: none;">
                <div class="accordion-group">
                    <div class="accordion-heading" style="background-color: rgb(255,237,135);">
                        <div class="accordion-toggle" style="cursor: auto;">
                            <a style="background: transparent;" data-toggle="collapse"
                                data-parent="#demo-accordion-01" href="#collapse-01">
                                <span class="btn" style="background: transparent; border: 0px;" ng-click="expendCollapse()"><i id="collapseBtn" class="icon-collapse-down"></i></span>
                            </a>
                            <span style="background: transparent;">过滤器 总费用 > 7.00, 总费用 > 50.00, 总费用 > 30.00</span>
                            <span style="background: transparent; float: right;"><a id="closeFilterLink" ng-click="closeFilterCollapse()" style="cursor: pointer; background: transparent; font-size: 25px; color: rgb(51,84,127);">×</a></span>
                        </div>

                    </div>
                    <div id="collapse-01" class="collapse" style="background-color: rgb(255,246,195);">
                        <div class="accordion-inner" style="padding: 0px;">

                            <div class="widget-content form-container">
                                <form class="form-horizontal">

                                    <div ng-repeat="filter in filters" id="input_cloning_template{{filter.id}}" class="control-group" style="border-top: 0px; border-bottom: 1px solid #ccc; padding: 5px 20px 5px 20px; margin: 0px;" idtemplate="input_cloning_template">
                                        <div class="btn-toolbar" style="margin: 0px; float: left;">
                                            <div class="btn-group">
                                                <button class="btn dropdown-toggle" data-toggle="dropdown">
                                                    <span>总费用{{filter.id}}</span>
                                                    <span class="caret"></span>
                                                </button>
                                                <ul class="dropdown-menu" style="">
                                                    <li title="" style="cursor: pointer;">态状</li>
                                                    <li title="" style="cursor: pointer;">总费用</li>
                                                </ul>
                                            </div>
                                            <div class="btn-group">
                                                <button class="btn dropdown-toggle" data-toggle="dropdown">
                                                    <span>></span>
                                                    <span class="caret"></span>
                                                </button>
                                                <ul class="dropdown-menu" style="">
                                                    <li title="" style="cursor: pointer;">></li>
                                                    <li title="" style="cursor: pointer;"><</li>
                                                </ul>
                                            </div>
                                            <input type="text" id="input_cloning_0_input" name="text[0]" idtemplate="input_cloning_%23index%23_input" nametemplate="text%5B%23index%23%5D">
                                        </div>
                                        <span class="close" id="input_cloning_remove_current" ng-click="removeFilter($index)" style="display: inline; float: right;">×</span>
                                    </div>

                                    <div class="control-group" style="border-top: 0px; padding-top: 15px; padding-left: 22px;">
                                        <div style="float: left;">
                                            <span class="btn btn-mini" id="input_cloning_add" ng-click="addFilter()"><i class="icon-plus"></i></span><span>&nbsp;Add another</span>
                                        </div>
                                        <div style="margin-left: 10px; float: left;">

                                            <label class="checkbox">
                                                <input type="checkbox">
                                                Save Filter
                                            </label>
                                        </div>

                                        <div style="margin-left: 10px; float: left;">
                                            <label class="span1" style="padding-right: 8px;">VisableTo</label>
                                            <select id="userDropdown" class="span2" style="margin-top: -3px;">
                                                <option>Me</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div style="margin: 0 0 5px 20px;">
                                        <button type="submit" class="btn btn-primary">Apply</button>
                                        <button class="btn" ng-click="closeFilterCollapse()">Close</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <section class="gridStyle" data-ng-grid="gridOptions"></section>
        </div>
    </section>
</section>
